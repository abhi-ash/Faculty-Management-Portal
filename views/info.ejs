
<%- include("partials/header1"); -%>
<table class="content-table center">
    <thead>   
        <tr>
            <th>Department Id</th>
            <th>Department Name</th>
        </tr>
    </thead>
    <tbody>
        <% department.forEach(function(dep){ %>
            <tr>
                <td><%= dep.department_id %> </td>
                <td><%= dep.department_name %></td>
            </tr>
            <% }) %>
    </tbody>
</table>
<br>
<table class="content-table center">
    <thead>   
        <tr>
            <th>Employee Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Id</th>
            <th>Department Id</th>
            <th>Date of joining</th>
            <th>Position</th>
        </tr>
    </thead>
    <tbody>
        <% employee.forEach(function(employe){ %>
            <tr>
                <td><%= employe.employee_id %> </td>
                <td><%= employe.first_name %></td>
                <td> <%= employe.last_name %> </td>
                <td><%= employe.email_id %> </td>
                <td><%= employe.department_id %> </td>
                <td><%= employe.date_of_joining %> </td>
                <td><%= employe.role %> </td>
            </tr>
            <% }) %>
    </tbody>
</table>

<%- include("partials/footer"); -%>
